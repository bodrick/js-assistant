{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "run - app/vscode-refactor",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/app/vscode-refactor"
      ],
      "outFiles": ["${workspaceFolder}/app/vscode-refactor/dist/**/*.js"]
    },
    {
      "name": "run(web) - app/vscode-refactor",
      "type": "pwa-extensionHost",
      "debugWebWorkerHost": true,
      "request": "launch",
      "args": [
        "--disable-extensions",
        "--extensionDevelopmentPath=${workspaceFolder}/app/vscode-refactor",
        "--extensionDevelopmentKind=web"
      ],
      "outFiles": ["${workspaceFolder}/app/vscode-refactor/dist/**/*.js"]
    }
  ]
}
